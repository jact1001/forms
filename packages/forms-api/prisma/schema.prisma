// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // url      = env("DATABASE_URL")
  url      = "file:./forms.db"
}

model Access {
  userId   String @id
  userName String
}

model Section {
  id           String @id
  section_name String
  //author       Form   @relation(fields: [section_name], references: [form_name])
}

model Form {
  id        String    @id
  form_name String    @unique
  state     String
  author    String
  //sections  Section[]
}
